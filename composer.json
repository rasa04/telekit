{
    "name": "rasa/telekit",
    "type": "project",
    "description": "Library for creating telegram bots",
    "homepage": "https://matesite.uz",
    "keywords": ["telekit", "rasa"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdurasul Khuzhamberdiyev",
            "email": "abdurasul2004.04@gmail.com",
            "homepage": "https://matesite.uz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "rasa/telekit-core":  "dev-main",
        "vlucas/phpdotenv": "^5.5",
        "ext-curl": "*",
        "ext-pdo": "*",
        "eftec/bladeone": "^4.9",
        "openai-php/client": "^0.8.1",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Database\\": "database/",
            "Responses\\": "app/responses/",
            "Middlewares\\": "app/middlewares",
            "Services\\": "app/services"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
